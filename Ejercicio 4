            int Ni = 0, Pat = 0, Es = 0;
            double PagMat=50000;
            string Nom;
            Console.WriteLine("Ingrese el numero de la inscripcion");
            Ni = Int32.Parse(Console.ReadLine());
            Console.WriteLine("Ingrese el nombr");
            Nom = Console.ReadLine();
            Console.WriteLine("Ingrese el patrimonio");
            Pat = Int32.Parse(Console.ReadLine());
            Console.WriteLine("Ingrese el estrato social");
            Es = Int32.Parse(Console.ReadLine());

    
            if (Pat > 2000000 && Es > 3)
            {
                PagMat = PagMat + 0.03 * Pat;
            }

            Console.WriteLine("El estudiante con numero de insc" + Ni
                + "y nombre " + Nom + "debe pagar: $ " + PagMat);
           // Console.ReadKey();
        }
    }
}
